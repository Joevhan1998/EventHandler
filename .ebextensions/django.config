container_commands:
  01_migrate:
    command: "python EventHandler/manage.py migrate"
    leader_only: true
  02_collectstatic:
    command: "python EventHandler/manage.py collectstatic --noinput"

option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "EventHandler.settings"
    "PYTHONPATH": "/opt/python/current/app/EventHandler:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: EventHandler/EventHandler/wsgi.py